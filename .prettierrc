{
    "printWidth": 160,
    "singleQuote": true,
    "semi": false,
    "svelteSortOrder": "options-scripts-markup-styles",
    "tabWidth": 4,
    "endOfLine": "lf",
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "plugins": ["prettier-plugin-svelte"],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}
